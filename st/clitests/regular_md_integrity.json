{"desc": "Default system test plan for Metada Integrity check",
 "tests": [
     {"desc": "Disable FI di_metadata_bucket_or_object_corrupted",
      "op": "disable-fi", "fi": "di_metadata_bucket_or_object_corrupted"},
     {"desc": "Create bucket test-bucket-1",
      "op": "create-bucket", "bucket": "test-bucket-1", "expect": true},
     {"desc": "Put object object1 to bucket test-bucket-1",
      "op": "put-object", "bucket": "test-bucket-1", "key": "object1", "expect": true},
     {"desc": "Head-object request for previous object",
      "op": "head-object", "bucket": "test-bucket-1", "key": "object1", "expect": true},
     {"desc": "Get-object request for previous object",
      "op": "get-object", "bucket": "test-bucket-1", "key": "object1", "expect": true},
     {"desc": "Copy-object request for previous object",
      "op": "copy-object", "bucket": "test-bucket-1", "key": "object12", "copy-source": "test-bucket-1/object1", "expect": true},
     {"desc": "Re-copy-object to the same dest",
      "op": "copy-object", "bucket": "test-bucket-1", "key": "object12", "copy-source": "test-bucket-1/object1", "expect": true},
     {"desc": "Head-object for copied object",
      "op": "head-object", "bucket": "test-bucket-1", "key": "object12", "expect": true},
     {"desc": "Enable FI di_metadata_bucket_or_object_corrupted",
      "op": "enable-fi", "fi": "di_metadata_bucket_or_object_corrupted"},
     {"desc": "Head-object request for previous object",
      "op": "head-object", "bucket": "test-bucket-1", "key": "object1", "expect": false},
     {"desc": "Get-object request for previous object",
      "op": "get-object", "bucket": "test-bucket-1", "key": "object1", "expect": false},
     {"desc": "Try to recreate object object1",
      "op": "put-object", "bucket": "test-bucket-1", "key": "object1", "expect": false},
     {"desc": "Try to delete-object request for previous object",
      "op": "delete-object", "bucket": "test-bucket-1", "key": "object1", "expect": false},
     {"desc": "Copy-object to new dest should fail",
      "op": "copy-object", "bucket": "test-bucket-1", "key": "object13", "copy-source": "test-bucket-1/object1", "expect": false},
     {"desc": "Copy-object to the existing dest should fail",
      "op": "copy-object", "bucket": "test-bucket-1", "key": "object12", "copy-source": "test-bucket-1/object1", "expect": false},
     {"desc": "Disable FI di_metadata_bucket_or_object_corrupted",
      "op": "disable-fi", "fi": "di_metadata_bucket_or_object_corrupted"},
     {"desc": "Delete-object request for previous object",
      "op": "delete-object", "bucket": "test-bucket-1", "key": "object1", "expect": true},
     {"desc": "Delete-object request for previous object",
      "op": "delete-object", "bucket": "test-bucket-1", "key": "object12", "expect": true},
     {"desc": "Delet bucket test-bucket-1",
      "op": "delete-bucket", "bucket": "test-bucket-1", "expect": true}
 ]}
